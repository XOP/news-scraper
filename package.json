{
  "name": "news-scraper",
  "version": "0.5.3",
  "description": "news-scraper",
  "main": "index.js",
  "scripts": {
    "prebuild": "trash dist && mkdir dist",
    "build": "babel src --out-dir dist && npm run static",
    "fetch": "node tools/git-fetch-resources.js",
    "test": "node tests",
    "lint-es5": "eslint tests/ tools/",
    "lint-es6": "eslint src",
    "lint": "npm run lint-es5 && npm run lint-es6",
    "precommit": "npm run lint",
    "prepublish": "npm run precommit && npm run build",
    "prestart": "npm run fetch",
    "start": "node dist",
    "dev": "node dist",
    "debug": "cross-env DEBUG=true node dist",
    "index": "node tools/re-render-index.js",
    "deploy": "$(./bin/deploy.js)",
    "add": "node tools/add-sources",
    "static": "gulp build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XOP/news-scraper.git"
  },
  "keywords": [
    "scraper",
    "parser",
    "articles",
    "news",
    "json",
    "markup",
    "html",
    "nightmare",
    "electron"
  ],
  "author": "Evgeny Khoroshilov <stewiekillsloiss@gmail.com> (https://github.com/XOP)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/XOP/news-scraper/issues"
  },
  "homepage": "https://github.com/XOP/news-scraper#readme",
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-cli": "^6.9.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "browser-sync": "^2.14.0",
    "cssnano": "^3.7.3",
    "del": "^2.2.1",
    "eslint": "^2.11.0",
    "eslint-config-xop": "^1.2.0",
    "gulp": "^3.9.1",
    "gulp-concat-css": "^2.3.0",
    "gulp-load-plugins": "^1.2.4",
    "gulp-postcss": "^6.1.1",
    "gulp-sass": "^2.3.2",
    "husky": "^0.11.4",
    "merge2": "^1.0.2",
    "postcss-scss": "^0.1.9",
    "run-sequence": "^1.2.2"
  },
  "dependencies": {
    "bluebird": "^3.4.0",
    "cheerio": "^0.20.0",
    "cross-env": "^1.0.8",
    "date-format": "0.0.2",
    "fs-extra": "^0.30.0",
    "git-wrapper": "^0.1.1",
    "is": "^3.1.0",
    "lodash.differenceby": "^4.6.0",
    "lodash.intersectionby": "^4.5.0",
    "lodash.isequal": "^4.2.0",
    "lodash.merge": "^4.5.1",
    "log-util": "^1.1.1",
    "nightmare": "^2.6.1",
    "normalize.css": "^4.2.0",
    "prompt": "^1.0.0",
    "tape": "^4.5.1",
    "trash-cli": "^1.3.0",
    "yamljs": "^0.2.7"
  }
}
