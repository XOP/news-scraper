<section class="news-section">
    <div class="news-section__heading">
        {{#> heading-2}}
            {{title}}
        {{/heading-2}}
    </div>
    <div class="news-section__url">{{url}}</div>
    <div class="news-section__content">
        {{#each data}}
            {{> news-item }}
        {{/each}}
    </div>
</section>
